{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "/(.*\\.(?:json|ico|png|jpg|jpeg|svg))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "installCommand": "npm install"
}
